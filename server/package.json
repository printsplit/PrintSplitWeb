{
  "name": "printsplit-server",
  "version": "1.0.0",
  "description": "PrintSplit Web API Server",
  "main": "dist/api/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/api/index.ts",
    "build": "tsc",
    "start": "node dist/api/index.js",
    "worker": "node dist/worker/index.js",
    "worker:dev": "NODE_ENV=development tsx watch src/worker/index.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "stl",
    "3d-printing",
    "splitting"
  ],
  "author": "Shea Silverman",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "minio": "^7.1.3",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "archiver": "^6.0.1",
    "dotenv": "^16.3.1",
    "manifold-3d": "^3.2.1",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/archiver": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
